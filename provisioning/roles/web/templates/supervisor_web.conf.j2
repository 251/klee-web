[program:titb_flask]
command=/titb_flask/env/bin/uwsgi -s /tmp/uwsgi.sock -w web:app -H /titb_flask/env/ --chmod-socket=666 {{ "--py-autoreload=1" if development}}
directory=/titb/web/
autostart=true
autorestart=true
stdout_logfile=/titb_flask/logs/uwsgi.log
redirect_stderr=true
user=web
stopsignal=QUIT