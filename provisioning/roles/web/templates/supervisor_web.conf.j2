[program:titb_flask]
command=/titb_flask/env/bin/uwsgi -s /tmp/uwsgi.sock -w python.web.web:app -H /titb_flask/env/ --chmod-socket=666 {{ "--py-autoreload=1 --catch-exceptions" if development else ""}}
directory=/titb/
autostart=true
autorestart=true
stdout_logfile=/titb_flask/logs/uwsgi.log
redirect_stderr=true
user=web
stopsignal=QUIT
environment=BROKER_URL="{{ rabbitmq_broker_url }}"{{ ',DEVELOPMENT="1"' if development else ''}}